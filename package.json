    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "openai": "^4.20.1",
    "bullmq": "^5.0.0",
    "winston": "^3.11.0"
